<?xml version="1.0" encoding="utf-8" ?>
<!-- 
PLANTILLA de Obfuscar
Documentación completa en 
https://docs.obfuscar.com/getting-started/configuration.html
-->
<Obfuscator>
	<!-- CAMBIA RUTA donde están tus ensablados -->
	<Var name="InPath" value="." />
	<!-- CAMBIA RUTA donde quieres meter los ofuscados resultantes -->
	<Var name="OutPath" value=".\Ofuscados" />
	<Var name="LogFile" value=".ofuscacion-log.txt" />
	<Var name="RenameProperties" value="false" />
	<Var name="RenameEvents" value="false" />
	<Var name="RenameFields" value="false" />
	<Var name="KeepPublicApi" value="false" />
	<Var name="HidePrivateApi" value="false" />
	<Var name="UseUnicodeNames" value="false" />
	<Var name="HideStrings" value="false" />
	<Var name="OptimizeMethods" value="false" />
	<Var name="SuppressIldasm" value="false" />
	<!-- CAMBIA RUTA Y NOMBRE de los ensamblados finales -->
	<Module file="$(InPath)\ArmaduraLosaRevit.Model.dll" />
  <Module file="$(InPath)\ArmaduraLosaRevit1.0000.dll" />
  <Module file="$(InPath)\UtilApiRevit.dll" />
</Obfuscator>